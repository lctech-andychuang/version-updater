    "@eslint/js": "^9.17.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.17.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  }
